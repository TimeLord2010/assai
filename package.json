{
  "name": "assai",
  "version": "0.4.4",
  "repository": {
    "url": "https://github.com/TimeLord2010/assai",
    "type": "git"
  },
  "description": "A simple mongodb wrapper to make mongo even easier to work with.",
  "main": "index.mjs",
